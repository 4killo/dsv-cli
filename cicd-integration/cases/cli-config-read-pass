CLI config (cicd-integration/.thy.yml):
default:
  auth:
    client:
      id: {{.CLIENT_ID}}
      secret: {{.DSV_CLIENT_SECRET}}
    password: {{.DSV_USER_PASSWORD}}
    type: password
    username: {{.USER_NAME}}
  domain: {{.DEV_DOMAIN}}
  http: https
  store:
    type: file
  tenant: {{.TEST_TENANT}}
local:
  auth:
    password: {{.DSV_USER_PASSWORD}}
    type: password
    username:  {{.USER_NAME}}
  domain: {{.LOCAL_DOMAIN}}
  http: http
  tenant: {{.TEST_TENANT}}